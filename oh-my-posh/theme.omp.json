{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "console_title_template": "{{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#1e1e2e",
          "background": "#89b4fa",
          "template": " {{ .Path }} ",
          "properties": {
            "style": "folder"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#1e1e2e",
          "background": "#a6e3a1",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#f9e2af{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f38ba8{{ end }}",
            "{{ if gt .Ahead 0 }}#89dceb{{ end }}",
            "{{ if gt .Behind 0 }}#f5c2e7{{ end }}"
          ],
          "template": " {{ .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        },
        {
          "type": "flutter",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#1e1e2e",
          "background": "#89dceb",
          "template": " \ue28e {{ .Full }} "
        },
        {
          "type": "dart",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#1e1e2e",
          "background": "#94e2d5",
          "template": " \ue798 {{ .Full }} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "node",
          "style": "plain",
          "foreground": "#a6e3a1",
          "template": "\ue718 {{ .Full }} ",
          "properties": {
            "display_mode": "files"
          }
        },
        {
          "type": "go",
          "style": "plain",
          "foreground": "#89dceb",
          "template": "\ue627 {{ .Full }} ",
          "properties": {
            "display_mode": "files"
          }
        },
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "#f9e2af",
          "template": "\uf520 {{ .FormattedMs }} ",
          "properties": {
            "threshold": 500
          }
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "#cdd6f4",
          "template": "\uf017 {{ .CurrentDate | date .Format }} ",
          "properties": {
            "time_format": "15:04"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#f38ba8{{ end }}",
            "{{ if eq .Code 0 }}#a6e3a1{{ end }}"
          ],
          "template": "\u276f "
        }
      ]
    }
  ]
}
